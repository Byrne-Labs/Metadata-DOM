<Project ToolsVersion="15.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Import Project="$(MSBuildExtensionsPath)\$(MSBuildToolsVersion)\Microsoft.Common.props" />
  <PropertyGroup>
    <TargetFramework>netstandard1.5</TargetFramework>
    <id>ByrneLabs.Commons.MetadataDom</id>
    <version>1.0.0-rc1</version>
    <authors>Jonathan Byrne</authors>
    <owners>Byrne Labs</owners>
    <licenseUrl>https://github.com/Byrne-Labs/Metadata-DOM/blob/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/Byrne-Labs/Metadata-DOM/</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/Byrne-Labs/Metadata-DOM/master/beaker.png</iconUrl>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <releaseNotes>Initial release</releaseNotes>
    <description>A domain object model (DOM) wrapper of the .NET Core System.Reflection.Metadata classes</description>
    <copyright>Copyright ©2016 Byrne Labs</copyright>
    <tags>"System.Reflection.Metadata" Reflection ".NET Core" ".NET Standard"</tags>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="**\*.cs" />
    <EmbeddedResource Include="**\*.resx" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="JetBrains.Annotations">
      <Version>10.2.1</Version>
    </PackageReference>
    <PackageReference Include="NETStandard.Library">
      <Version>1.5.0-rc2-24027</Version>
    </PackageReference>
    <PackageReference Include="Microsoft.NET.Sdk">
      <Version>1.0.0-alpha-20161104-2</Version>
      <PrivateAssets>All</PrivateAssets>
    </PackageReference>
    <PackageReference Include="System.Reflection.Metadata">
      <Version>1.3.0</Version>
    </PackageReference>
  </ItemGroup>
  <Import Project="$(MSBuildToolsPath)\Microsoft.CSharp.targets" />
</Project>